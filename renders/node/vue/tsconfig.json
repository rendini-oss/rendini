{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "declaration": true
  },
  "include": ["./**/*.ts", "./**/*.vue"],
  "exclude": ["node_modules", "dist"]
}
