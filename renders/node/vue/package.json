{
  "name": "rendini-renders-vue",
  "version": "0.1.0",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "dev": "tsc --watch",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "copy-assets": "make-dir dist/graphql && cpy \"graphql/*.html\" dist/graphql/"
  },
  "dependencies": {
    "express": "^5.1.0",
    "graphiql": "^4.1.2",
    "graphql": "^16.11.0",
    "vue": "^3.5.17"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@vue/tsconfig": "^0.5.1",
    "cpy-cli": "^5.0.0",
    "make-dir-cli": "^4.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  }
}
